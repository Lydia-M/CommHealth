<script src="//underscorejs.org/underscore-min.js"></script>
<script src="//maps.google.com/maps/api/js?key=AIzaSyDeSxj8ywCFVVCUZyskjZgIEK_mlPDUUU8"></script>
<script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
<script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->

<p id="notice"><%= notice %></p>

<h1>Incidents</h1>

<div style='width: 1080px;'>
  <div id="map" style='width: 1080px; height: 400px;'></div>
</div>

<script>
    new google.maps.Map(document.getElementById('map'), {
          center: {lat: 30.455, lng: -84},
          zoom: 8
        });
</script>

<table>
  <thead>
    <tr>
      <th>Category</th>
      <th>Street address 1</th>
      <th>Street address 2</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Occurred on</th>
      <th></th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
 
    </tr>
  </thead>

  <tbody>
    <% @incidents.each do |incident| %>
      <tr>
        <td><%= incident.category %></td>
        <td><%= incident.street_address_1 %></td>
        <td><%= incident.street_address_2 %></td>
        <td><%= incident.city %></td>
        <td><%= incident.state %></td>
        <td><%= incident.zip %></td>
        <td><%= incident.occurred_on %></td>
        <td><%= incident.latitude %></td>
        <td><%= incident.longitude %></td>
        <td><%= link_to 'Show', incident %></td>
        <td><%= link_to 'Edit', edit_incident_path(incident) %></td>
        <td><%= link_to 'Destroy', incident, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Incident', new_incident_path %>

